{
	"compilerOptions": {
		"alwaysStrict": true,
		"importHelpers": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"target": "es5",
		"lib": ["es2015"],
		"strictNullChecks": true,
		"outDir": "dist"
	}
}
